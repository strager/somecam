{
	"name": "somecam",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=25"
	},
	"scripts": {
		"dev": "node --env-file-if-exists=.env backend/main.ts",
		"build": "vite build && vite build --ssr pdf-entry.ts --outDir dist-ssr",
		"analyze-client-bundle": "npm run build",
		"typecheck": "tsc -p tsconfig.backend.json && vue-tsc --noEmit -p tsconfig.frontend.json",
		"test": "vitest run",
		"bench": "vitest bench --run",
		"lint": "eslint .",
		"fix": "eslint --fix .",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@vue/server-renderer": "^3.5.28",
		"altcha-lib": "^1.4.1",
		"better-sqlite3": "^12.6.2",
		"express": "^5.1.0",
		"openapi-backend": "^5.15.0",
		"posthog-js-lite": "^3.3.0",
		"vue": "^3.5.13",
		"vue-router": "^4.5.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@types/better-sqlite3": "^7.6.13",
		"@types/express": "^5.0.3",
		"@types/node": "^24.0.0",
		"@vitejs/plugin-vue": "^6.0.1",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-vue": "^10.7.0",
		"happy-dom": "^20.6.1",
		"msw": "^2.12.10",
		"prettier": "3.8.1",
		"rollup-plugin-visualizer": "^6.0.5",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.55.0",
		"vite": "^7.1.2",
		"vitest": "^3.2.4",
		"vue-tsc": "^2.2.8"
	}
}
